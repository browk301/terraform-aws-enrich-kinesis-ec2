# Config Hash: ${config_hash}
enrich {
  streams {
    in {
      raw = "${in_stream_name}"
    }
    out {
      enriched = "${enriched_stream_name}"
      bad = "${bad_stream_name}"
      partitionKey = ""
    }
    sourceSink {
      enabled = kinesis
      region = "${region}"
      aws {
        accessKey = iam
        secretKey = iam
      }
      maxRecords = 10000
      initialPosition = "${initial_position}"
      initialTimestamp = ""
      backoffPolicy {
        minBackoff = 50
        maxBackoff = 500
      }
      disableCloudWatch = true
    }
    buffer {
      byteLimit = 1000000
      recordLimit = 500
      timeLimit = 50
    }
    appName = "${app_name}"
  }
}
